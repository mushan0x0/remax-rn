{
  "name": "remax-rn",
  "version": "1.0.7",
  "description": "remax-rn",
  "main": "src/index",
  "module": "src/index",
  "types": "src/index",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "android",
    "ios",
    "cpp",
    "remax-rn.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest --verbose ./src",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/mushan0x0/remax-rn",
  "author": "mushan <mushan0x0@gmail.com> (https://github.com/mushan0x0)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mushan0x0/remax-rn/issues"
  },
  "homepage": "https://github.com/mushan0x0/remax-rn#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@ant-design/react-native": "^4.2.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@kqinfo/ui": "^1.8.5",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-clipboard/clipboard": "^1.8.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-navigation/native": "5.x",
    "@react-navigation/stack": "^6.0.11",
    "@release-it/conventional-changelog": "^2.0.0",
    "@remax/plugin-less": "^1.0.0",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^26.0.0",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-polyfill-corejs3": "^0.2.4",
    "classnames": "^2.2.6",
    "commitlint": "^11.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-to-react-native": "^3.0.0",
    "css-viewport-units-transform": "^0.10.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gcoord": "^0.3.2",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "jest-css-modules": "^2.1.0",
    "jscpd": "^3.3.19",
    "lint-staged": "^9.5.0",
    "metro-react-native-babel-preset": "^0.70.0",
    "parsec-codegen": "^0.0.0-alpha.6",
    "parsec-hooks": "^0.0.157",
    "pod-install": "^0.1.0",
    "postcss-unit-transforms": "^0.0.0-alpha.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "qs": "^6.10.1",
    "react": "16.13.1",
    "react-dom": "^17.0.2",
    "react-native": "^0.67.4",
    "react-native-builder-bob": "^0.18.2",
    "react-native-color-matrix-image-filters": "^5.2.10",
    "react-native-get-location": "^2.1.2",
    "react-native-image-crop-picker": "^0.36.2",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-svg": "^12.1.1",
    "react-native-svg-web": "^1.0.9",
    "react-test-renderer": "^17.0.2",
    "release-it": "^14.2.2",
    "semver": "^7.3.2",
    "typescript": "^4.1.3",
    "validate-commit-msg": "^2.14.0"
  },
  "peerDependencies": {
    "@ant-design/react-native": "*",
    "@kqinfo/ui": "*",
    "@react-native-async-storage/async-storage": "*",
    "@react-native-clipboard/clipboard": "*",
    "@react-navigation/native": "*",
    "@react-navigation/stack": "*",
    "babel-plugin-polyfill-corejs3": "*",
    "css-to-react-native": "*",
    "css-viewport-units-transform": "*",
    "gcoord": "*",
    "parsec-hooks": "*",
    "qs": "*",
    "react": "*",
    "react-native": "*",
    "react-native-color-matrix-image-filters": "*",
    "react-native-get-location": "*",
    "react-native-image-crop-picker": "*",
    "react-native-image-zoom-viewer": "*",
    "react-native-svg": "*"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      "remax/(.*)$": "<rootDir>/src/$1",
      "^(react-native)$": "<rootDir>/node_modules/react-native",
      "^(react)$": "<rootDir>/node_modules/react",
      "^(classnames)$": "<rootDir>/src/utils/classNames",
      "react-native-svg": "<rootDir>/node_modules/react-native-svg-web",
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(.*(rn-color-matrices|@react-native|react-native|concat-color-matrices|@kqinfo|@sentry/react-native|react-native-svg-web).*))"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "setupFiles": [
      "./setup.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {}
}
